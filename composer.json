{
    "name": "solumdesignum/recomposer",
    "description": "A Laravel package to decompose your installed packages, their dependencies, your app & server environment",
    "license": "Proprietary software",
    "type": "project",
    "authors": [
        {
            "name": "Oskars Germovs",
            "email": "oskars_germovs@inbox.lv",
            "homepage": "https://solum-desigmum.eu",
            "role": "Developer"
        },
        {
            "name": "Harish Toshniwal",
            "email": "harish@lubus.in"
        }
    ],
    "homepage": "https://github.com/solumdesignum/recomposer",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "illuminate/support": "^5.0 | ^5.5 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/view": "^5.0 | ^5.5 || ^6.0 || ^7.0 || ^8.0",
        "gabrielelana/byte-units": "^0.5"
    },
    "autoload": {
        "psr-4": {
            "SolumDeSignum\\ReComposer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolumDeSignum\\ReComposer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SolumDeSignum\\ReComposer\\ReComposerServiceProvider"
            ],
            "aliases": {
                "ReComposer": "SolumDeSignum\\ReComposer\\Facades\\ReComposerFacade"
            },
            "branch-alias": {
                "dev-master": "1.0-dev"
            }
        }
    }
}
